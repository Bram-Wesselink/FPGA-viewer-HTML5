{
  "name": "nextpnr-viewer",
  "description": "A HTML 5 Canvas based viewer for nextpnr place and route visualizations.",
  "version": "0.7.0",
  "license": "MIT",
  "contributors": [
    "Bram Wesselink <wesselinkbram@gmail.com>",
    "Mike Almeloo <git@mikealmel.ooo>"
  ],
  "repository": "github:EDAcation/nextpnr-viewer",
  "keywords": [
    "nextpnr",
    "viewer",
    "place",
    "route",
    "visualization"
  ],
  "source": "src/index.ts",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "package.json"
  ],
  "scripts": {
    "check": "tsc --noEmit",
    "build-wasm": "wasm-pack build --release --target web",
    "build": "npm run-script build-wasm && rollup -c",
    "prepublish": "npm run build"
  },
  "dependencies": {
    "nextpnr-renderer": "file:./pkg"
  }
}
